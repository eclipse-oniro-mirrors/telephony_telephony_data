{
  "module": {
    "name": "telephonydataentry",
    "type": "entry",
    "srcEntrance": "./ets/Application/TelephonyDataAbilityStage.ets",
    "description": "$string:app_name",
    "mainElement": "MainAbility",
    "compressNativeLibs": true,
    "deviceTypes": [
      "default",
      "tablet",
      "wearable"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [],
    "extensionAbilities": [
      {
        "name": "OpKeyAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "readPermission": "ohos.permission.GET_TELEPHONY_STATE",
        "writePermission": "ohos.permission.SET_TELEPHONY_STATE",
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.opkeyability",
        "visible": true
      },
      {
        "name": "BackupRestoreWorkScheduler",
        "srcEntry": "./ets/WorkScheduler/BackupRestoreWorkScheduler.ets",
        "description": "backup db",
        "type": "workScheduler"
      },
      {
        "name": "BackupRestoreServiceAbility",
        "srcEntry": "./ets/ServiceAbility/BackupRestoreServiceAbility.ets",
        "type": "service",
        "exported": true,
        "description": "BackupRestoreService",
        "permissions": [
          "ohos.permission.READ_MESSAGES"
        ]
      },
      {
        "name": "PdpProfileAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "readPermission": "ohos.permission.GET_TELEPHONY_STATE",
        "writePermission": "ohos.permission.SET_TELEPHONY_STATE",
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.pdpprofileability",
        "visible": true
      },
      {
        "name": "SimAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "readPermission": "ohos.permission.GET_TELEPHONY_STATE",
        "writePermission": "ohos.permission.SET_TELEPHONY_STATE",
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.simability",
        "visible": true
      },
      {
        "name": "SmsMmsAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "permissions": [
          "ohos.permission.READ_MESSAGES"
        ],
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.smsmmsability",
        "visible": true
      },
      {
        "name": "GlobalParamsAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "readPermission": "ohos.permission.GET_TELEPHONY_STATE",
        "writePermission": "ohos.permission.SET_TELEPHONY_STATE",
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.globalparamsability",
        "visible": true
      },
      {
        "name": "OpkeyVersionAbility",
        "srcEntrance": "./ets/DataShareExtAbility/DataShareExtAbility.ts",
        "description": "dataability_description",
        "type": "dataShare",
        "uri": "datashare://com.ohos.opkeyversionability",
        "visible": true
      },
      {
        "name": "BackupExtensionAbility",
        "srcEntrance": "./ets/backup/BackupExtension.ets",
        "description": "$string:backup_restore",
        "label": "$string:backup_restore",
        "type": "backup",
        "visible": true,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
        "permissions": [
          "ohos.permission.READ_MESSAGES"
        ]
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      },
      {
        "name": "ohos.permission.PROTECT_SCREEN_LOCK_DATA"
      },
      {
        "name": "ohos.permission.READ_MESSAGES",
        "reason": "$string:permission_read_write_messages",
        "usedScene": {
          "abilities": [
            "SmsMmsAbility",
            "BackupRestoreServiceAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO"
      }
    ],
    "proxyData": [
      {
        "uri": "datashareproxy://com.ohos.telephonydataability/telephonydata",
        "requiredReadPermission": "ohos.permission.READ_MESSAGES",
        "requiredWritePermission": "ohos.permission.READ_MESSAGES",
        "metadata": {
          "name": "dataProperties",
          "resource": "$profile:proxy_data_share_config"
        }
      }
    ]
  }
}
